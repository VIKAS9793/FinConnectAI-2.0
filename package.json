{
  "name": "finconnect-ai",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "check": "node scripts/check-dependencies.js",
    "check:dependencies": "node scripts/check-dependencies.js",
    "maintain": "node scripts/lint-and-format.cjs",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "lint": "eslint .",
    "test": "vitest",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "server": "cd server && node index.js",
    "server:dev": "cd server && nodemon index.js",
    "dev:all": "node scripts/check-dependencies.js --dev && concurrently -n 'FRONTEND,BACKEND' -c 'bgBlue.bold,bgMagenta.bold' \"npm run dev\" \"npm run server:dev\"",
    "install:all": "npm install && cd server && npm install",
    "generate:demo-data": "node scripts/generate-demo-data.js",
    "setup": "node scripts/setup.js",
    "postinstall": "npm run setup",
    "start": "npm run dev:all"
  },
  "dependencies": {
    "@auth0/auth0-react": "^2.3.0",
    "@auth0/auth0-spa-js": "^2.1.3",
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-oauth2-jwt-bearer": "^1.2.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.22.2",
    "uuid": "^11.1.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/axios": "^0.14.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.15.21",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/ui": "^3.1.4",
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "faker": "^5.5.3",
    "globals": "^15.9.0",
    "jsdom": "^26.1.0",
    "nodemon": "^3.1.10",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "recharts": "^2.15.3",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^6.3.5",
    "vitest": "^3.1.4"
  }
}
